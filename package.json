  "description": "Node.js port of Tobias LÃ¼tke's Liquid template engine.",
  "version": "2.6.1",
  "license": "MIT",
  "homepage": "https://github.com/sirlantis/liquid-node",
  "bugs": "https://github.com/sirlantis/liquid-node/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/sirlantis/liquid-node.git"
  },
  "keywords": [
    "liquid",
    "template",
    "jinja"
  ],
  "directories": {
    "lib": "./lib"
  },
  "main": "./lib/index.js",
  "engines": {
    "node": ">= 0.10"
  },
  "dependencies": {
    "native-or-bluebird": "~1.2.0",
    "strftime": "~0.9.2"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2017": "^1.4.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "~3.2.0",
    "chai-as-promised": "~5.1.0",
    "coffee-script": "~1.10.0",
    "coffeelint": "^1.11.1",
    "coveralls": "^2.11.4",
    "decafjs": "^0.6.0",
    "glob": "^7.0.3",
    "jscoverage": "^0.6.0",
    "mocha": "~2.3.2",
    "mocha-lcov-reporter": "0.0.2",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register -R spec test",
    "compile": "rm -rf lib && coffee --output lib --map --compile src",
    "prepublish": "npm run precommit && npm run compile",
    "precommit": "npm test && npm run lint",
    "coverage": "npm run compile && LIQUID_NODE_COVERAGE=1 mocha --compilers js:babel-core/register -r jscoverage --reporter mocha-lcov-reporter test | coveralls",
    "coverage-report": "npm run compile && LIQUID_NODE_COVERAGE=1 mocha --compilers js:babel-core/register -r jscoverage --covout html test",
    "lint": "coffeelint src/** test/**"
  }
}
